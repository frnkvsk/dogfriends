{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",      
      "minLength": 5,
      "maxLength": 55,
      "examples": ["foo123"]
    },
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",      
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Michael"]
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Hueter"]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "title": "The Email Schema ",
      "default": "",
      "examples": ["michael@rithmschool.com"]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "examples": ["foo123"]
    },
    "city": {
      "$id": "/properties/city",
      "type": "string",
      "minLength": 2,
      "maxLength": 60,
      "title": "The City Schema ",
      "examples": ["Seattle"]
    },
    "state": {
      "$id": "/properties/state",
      "type": "string",
      "minLength": 2,
      "maxLength": 60,
      "title": "The State Schema ",
      "examples": ["WA"]
    },
    "country": {
      "$id": "/properties/country",
      "type": "string",
      "minLength": 2,
      "maxLength": 60,
      "title": "The Country Schema ",
      "examples": ["usa"]
    },
    "photo_id": {
      "$id": "/properties/photo",
      "type": "uuid",
      "title": "The Photo Schema ",
      "default": null,
      "examples": [
        "45ab-678a-123b-543z"
      ]
    }
  }
}
