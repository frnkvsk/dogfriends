{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["hueter"]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "minLength": 5,
      "maxLength": 55,
      "examples": ["foo123"]
    },
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Michael"]
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Hueter"]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "title": "The Email Schema ",
      "examples": ["michael@rithmschool.com"]
    },
    "photo_url": {
      "$id": "/properties/photo",
      "type": "string",
      "title": "The Photo Schema ",
      "default": "",
      "examples": [
        "https://avatars0.githubusercontent.com/u/13444851?s=460&v=4"
      ]
    },
    "admin": {
      "$id": "/properties/admin",
      "type": "boolean",
      "minLength": 4,
      "maxLength": 5,
      "title": "The admin Schema ",
      "default": false,
      "examples": [false]
    },
    "city": {
      "$id": "/properties/city",
      "type": "string",
      "minLength": 2,
      "maxLength": 60,
      "title": "The State Schema ",
      "examples": ["Seattle"]
    },
    "state": {
      "$id": "/properties/state",
      "type": "string",
      "minLength": 2,
      "maxLength": 60,
      "title": "The State Schema ",
      "examples": ["WA"]
    },
    "country": {
      "$id": "/properties/country",
      "type": "string",
      "minLength": 2,
      "maxLength": 60,
      "title": "The Country Schema ",
      "examples": ["usa"]
    }
  },
  "required": ["username", "password"]
}
